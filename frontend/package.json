  "proxy": "http://localhost:8084/",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.38.1",
    "http-proxy-middleware": "^2.0.6",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.5.4",
    "web-vitals": "^1.0.1"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^4.7.4",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "@turf/destination": "^6.5.0",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.1",
    "@types/react-csv": "^1.1.2",
    "@types/react-helmet": "^6.1.1",
    "deck.gl": "^8.4.13",
    "dotenv": "^8.2.0",
    "i18next": "^20.1.0",
    "i18next-browser-languagedetector": "^6.1.0",
    "i18next-http-backend": "^1.2.0",
    "mapbox-gl": "^2.1.1",
    "react": "^17.0.2",
    "react-csv": "^2.2.1",
    "react-dom": "^17.0.1",
    "react-error-overlay": "6.0.9",
    "react-google-places-autocomplete": "^3.3.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.15.5",
    "react-i18next": "^11.8.11",
    "react-map-gl": "^6.1.10",
    "react-map-gl-geocoder": "^2.1.6",
    "react-mapbox-gl-geocoder": "^1.1.0",
    "react-places-autocomplete": "^7.3.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redaxios": "^0.5.0",
    "worker-loader": "^3.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "prettier --write .",
    "lint:test": "prettier --check .",
    "build": "react-scripts build",
    "build:acceptance": "cp .env-acceptance .env; npm run build",
    "build:production": "cp .env-production .env; npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "import/no-webpack-loader-syntax": "off",
      "react-hooks/exhaustive-deps": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "jsx-a11y/anchor-has-content": "off",
      "no-useless-escape": "off",
      "no-throw-literal": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11",
      "not chrome < 51",
      "not safari < 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "react-error-overlay": "6.0.9",
    "react-mui-mapbox-geocoder": {
      "react-mapbox-gl-geocoder": "$react-mapbox-gl-geocoder",
      "react-dom": "$react-dom",
      "react": "$react"
    },
    "react-mapbox-gl-geocoder": {
      "react-dom": "$react-dom",
      "react": "$react"
    }
  }
}
